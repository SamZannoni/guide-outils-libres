            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="FT88 School" FONTSIZE="19" CH="Introduction "/>
                <para LINESPMode="1"/>
                <para LINESPMode="0"/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" CH="Le mouvement du logiciel libre est un mouvement social dont le but est d'obtenir"/>
                <para LINESPMode="0" MinWordTrack="1" MinGlyphShrink="1" MaxGlyphExtend="1" wordTrack="0.93"/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" CH="et de garantir une certaine liberté pour "/>
                <para ALIGN="0" LINESPMode="0" LINESP="15" wordTrack="1.12"/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" CH="les utilisateurices. Comme l'explique l'association Framasoft, l'objectif de ces pratique est de s'assurer que l'humain..e reste en maitrise de l'outil numérique"/>
                <ITEXT FONTSIZE="12" CH=" "/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" CH=": "/>
                <para ALIGN="0" LINESPMode="0" LINESP="15" wordTrack="1.02"/>
                <para LINESPMode="0"/>
                <ITEXT FONTSIZE="12" CH="“"/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" CH=" Le respect des libertés fondamentales des utilisatrices et utilisateurs, "/>
                <para ALIGN="1" LINESPMode="0" LINESP="15"/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" CH="garanties par des contrats légaux "/>
                <para ALIGN="1" LINESPMode="0" LINESP="15"/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" CH="(les licences libres), est au cœur "/>
                <para ALIGN="1" LINESPMode="0" LINESP="15"/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" CH="du mouvement libriste et permet "/>
                <para ALIGN="1" LINESPMode="0" LINESP="15"/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" CH="de s’assurer que l’humain reste "/>
                <para ALIGN="1" LINESPMode="0" LINESP="15"/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" CH="en maîtrise de l’outil numérique."/>
                <ITEXT FONTSIZE="12" CH=" ”"/>
                <para ALIGN="1" LINESPMode="0" LINESP="15"/>
                <para ALIGN="1" LINESPMode="0" LINESP="15"/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" CH="Dans nos pratiques créatives, les outils numériques sont au centre: logiciels "/>
                <para LINESPMode="0" wordTrack="1.15"/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" CH="de création, navigateur web, réseaux sociaux... Et nous les utilisons presque machinalement, sans remettre en question leur légitimité. Seulement, un certain petit nombre d'entre eux sont hégémo-niques dans nos appareils, notamment "/>
                <para LINESPMode="0" wordTrack="0.93"/>
