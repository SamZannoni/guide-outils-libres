                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" CH="de marché et nous soutenons la liberté "/>
                <para LINESP="15"/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" CH="de subvertir ses règles."/>
                <para LINESP="15"/>
                <para LINESP="15"/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" CH="* Nous pensons que la SIL Open Font License (OFL) est une bonne base "/>
                <para LINESP="15"/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" CH="pour apprendre aux gens comment "/>
                <para LINESP="15"/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" CH="créer et distribuer les typographies. "/>
                <para LINESP="15"/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" CH="Nous voulons populariser la typographie "/>
                <para LINESP="15"/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" CH="et le dessin de caractère et le rendre "/>
                <para LINESP="15"/>
