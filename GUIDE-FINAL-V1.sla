                <para LINESP="15"/>
                <para LINESP="15"/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" CH="* Nous pensons que rendre le dessin "/>
                <para LINESP="15"/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" CH="de caractère plus transparent accroît "/>
                <para LINESP="15"/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" CH="le dynamisme de la création contemporaine. "/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" SCALEH="96" CH="En ce sens, les utilisateur..ices "/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" SCALEH="99" CH="peuvent continuer à développer nos typo-graphies"/>
                <ITEXT FONT="DINdong &lt;3" FONTSIZE="12" CH=" en fonction de leurs point "/>
                <para LINESP="15" wordTrack="0.88"/>
